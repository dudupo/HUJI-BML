\begin_layout Standard
זה כמובן פשוט, אנו יודעים כי 
\begin_inset Formula 
\begin{align*}
P\left(y\right) & \sim\int P\left(y|\alpha\right)P\left(\alpha\right)=\int Gauss\left(K\alpha\right)P\left(\alpha\right)=\\
 & =\int Gauss\left(y\right)P\left(\alpha^{-1}\left(y\right)\right)|\frac{d\alpha}{dy}|dy=\int Gauss\left(y\right)P\left(\alpha^{-1}\left(y\right)\right)|K^{-1}|dy
\end{align*}

\end_inset


